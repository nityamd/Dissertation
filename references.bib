  journal = {Astronomy \& Astrophysics},
  author = {Bacon, R. and Brinchmann, J. and Richard, J. and Contini, T. and Drake, A. and Franx, M. and Tacchella, S. and Vernet, J. and Wisotzki, L. and Blaizot, J. and Bouch\'e, N. and Bouwens, R. and Cantalupo, S. and Carollo, C. M. and Carton, D. and Caruana, J. and Cl\'ement, B. and Dreizler, S. and Epinat, B. and Guiderdoni, B. and Herenz, C. and Husser, T.-O. and Kamann, S. and Kerutt, J. and Kollatschny, W. and Krajnovic, D. and Lilly, S. and Martinsson, T. and {Michel-Dansac}, L. and Patricio, V. and Schaye, J. and Shirazi, M. and Soto, K. and Soucail, G. and Steinmetz, M. and Urrutia, T. and Weilbacher, P. and {de Zeeuw}, T.},
