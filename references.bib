	File = {Tomczak et al. - 2014 - GALAXY STELLAR MASS FUNCTIONS FROM ZFOURGECANDELS.pdf:/Users/nitya/Zotero/storage/6EEYUS4I/Tomczak et al. - 2014 - GALAXY STELLAR MASS FUNCTIONS FROM ZFOURGECANDELS.pdf:application/pdf},
