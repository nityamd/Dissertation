@article{moustakas_primus:_2013,
	Abstract = {We measure the evolution of the stellar mass function (SMF) from z = 0--1 using multi-wavelength imaging and spectroscopic redshifts from the PRism MUlti-object Survey (PRIMUS) and the Sloan Digital Sky Survey (SDSS). From PRIMUS we construct an i {\textless} 23 flux-limited sample of âŒ40,000 galaxies at z = 0.2--1.0 over five fields totaling â5.5 deg2, and from the SDSS we select âŒ170,000 galaxies at z = 0.01--0.2 that we analyze consistently with respect to PRIMUS to minimize systematic errors in our evolutionary measurements. We find that the SMF of all galaxies evolves relatively little since z = 1, although we do find evidence for mass assembly downsizing; we measure a â30\% increase in the number density of âŒ1010 M galaxies since z â 0.6, and a 10\% change in the number density of all 1011 M galaxies since z â 1. Dividing the sample into star-forming and quiescent using an evolving cut in specific star formation rate, we find that the number density of âŒ1010 M star-forming galaxies stays relatively constant since z â 0.6, whereas the space density of 1011 M star-forming galaxies decreases by â50\% between z â 1 and z â 0. Meanwhile, the number density of âŒ1010 M quiescent galaxies increases steeply toward low redshift, by a factor of âŒ2--3 since z â 0.6, while the number of massive quiescent galaxies remains approximately constant since z â 1. These results suggest that the rate at which star-forming galaxies are quenched increases with decreasing stellar mass, but that the bulk of the stellar mass buildup within the quiescent population occurs around âŒ1010.8 M . In addition, we conclude that mergers do not appear to be a dominant channel for the stellar mass buildup of galaxies at z {\textless} 1, even among massive ( 1011 M ) quiescent galaxies.},
	Author = {Moustakas, John and Coil, Alison L. and Aird, James and Blanton, Michael R. and Cool, Richard J. and Eisenstein, Daniel J. and Mendez, Alexander J. and Wong, Kenneth C. and Zhu, Guangtun and Arnouts, St{\'e}phane},
	Doi = {10.1088/0004-637X/767/1/50},
	File = {Moustakas et al. - 2013 - PRIMUS CONSTRAINTS ON STAR FORMATION QUENCHING AN.pdf:/Users/nitya/Zotero/storage/QM4BZGRT/Moustakas et al. - 2013 - PRIMUS CONSTRAINTS ON STAR FORMATION QUENCHING AN.pdf:application/pdf},
