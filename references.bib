	Title = {{PRIMUS}: {CONSTRAINTS} {ON} {STAR} {FORMATION} {QUENCHING} {AND} {GALAXY} {MERGING}, {AND} {THE} {EVOLUTION} {OF} {THE} {STELLAR} {MASS} {FUNCTION} {FROM} \textit{z} = 0-1},
	Url = {http://stacks.iop.org/0004-637X/767/i=1/a=50?key=crossref.e75ff79433b7fecfce7a3a741cc18f65},
