@article{ilbert_mass_2013,
	Abstract = {We estimate the galaxy stellar mass function and stellar mass density for star-forming and quiescent galaxies with 0.2 {\textless} z {\textless} 4. We construct a large, deep (Ks {\textless} 24) sample of 220 000 galaxies selected using the new UltraVISTA DR1 data release. Our analysis is based on precise 30-band photometric redshifts. By comparing these photometric redshifts with 10,800 spectroscopic redshifts from the zCOSMOS bright and faint surveys, we find a precision of ÏÎz/(1+z) = 0.008 at i+ {\textless} 22.5 and ÏÎz/(1+z) = 0.03 at 1.5 {\textless} z {\textless} 4. We derive the stellar mass function and correct for the Eddington bias. We find a mass-dependent evolution of the global and starforming populations, with the low-mass end of the mass functions evolving more rapidly than the high-mass end. This mass-dependent evolution is a direct consequence of the star formation being ``quenched'' in galaxies more massive than M 1010.7â10.9M . For the mass function of the quiescent galaxies, we do not find any significant evolution of the high-mass end at z {\textless} 1; however we observe a clear flattening of the faint-end slope. From z âŒ 3 to z âŒ 1, the density of quiescent galaxies increases over the entire mass range. Their comoving stellar mass density increases by 1.6 dex between z âŒ 3 and z âŒ 1 and by less than 0.2 dex at z {\textless} 1. We infer the star formation history from the mass density evolution. This inferred star formation history is in excellent agreement with instantaneous star formation rate measurements at z {\textless} 1.5, while we find differences of 0.2 dex at z {\textgreater} 1.5 consistent with the expected uncertainties. We also develop a new method to infer the specific star formation rate from the mass function of star-forming galaxies. We find that the specific star formation rate of 1010â10.5M galaxies increases continuously in the redshift range 1 {\textless} z {\textless} 4. Finally, we compare our results with a semi-analytical model and find that these models overestimate the density of low mass quiescent galaxies by an order of magnitude, while the density of low-mass star-forming galaxies is successfully reproduced.},
	Author = {Ilbert, O. and McCracken, H. J. and Le F{\`e}vre, O. and Capak, P. and Dunlop, J. and Karim, A. and Renzini, M. A. and Caputi, K. and Boissier, S. and Arnouts, S. and Aussel, H. and Comparat, J. and Guo, Q. and Hudelot, P. and Kartaltepe, J. and Kneib, J. P. and Krogager, J. K. and Le Floc'h, E. and Lilly, S. and Mellier, Y. and Milvang-Jensen, B. and Moutard, T. and Onodera, M. and Richard, J. and Salvato, M. and Sanders, D. B. and Scoville, N. and Silverman, J. D. and Taniguchi, Y. and Tasca, L. and Thomas, R. and Toft, S. and Tresse, L. and Vergani, D. and Wolk, M. and Zirm, A.},
	Doi = {10.1051/0004-6361/201321100},
	File = {Ilbert et al. - 2013 - Mass assembly in quiescent and star-forming galaxi.pdf:/Users/nitya/Zotero/storage/TA99SLJY/Ilbert et al. - 2013 - Mass assembly in quiescent and star-forming galaxi.pdf:application/pdf},
	Issn = {0004-6361, 1432-0746},
	Journal = {Astronomy \& Astrophysics},
	Language = {en},
	Month = aug,
	Pages = {A55},
	Title = {Mass assembly in quiescent and star-forming galaxies since \textit{z} â 4 from {UltraVISTA}},
	Url = {http://www.aanda.org/10.1051/0004-6361/201321100},
	Urldate = {2018-04-06},
	Volume = {556},
	Year = {2013},
